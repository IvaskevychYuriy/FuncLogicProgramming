{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    },
    "Console": {
      "IncludeScopes": true
    }
  },
  "AllowedHosts": "*",
  "Configuration": {
    "TrelloAPIUrl": "https://api.trello.com/1/"
  },
  "LUIS": {
    "APIUrl": ""
  },
  "MemoryCache": {
    "ExpirationScanMinutes": 10,
    "SizeLimit": 100
  },
  "ContextEntry": {
    "Size": 1,
    "ExpirationMinutes": 60
  }
}
