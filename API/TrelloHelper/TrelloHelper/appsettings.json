{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    },
    "Console": {
      "IncludeScopes": true
    }
  },
  "AllowedHosts": "*",
  "Configuration": {
    "LUIS": {
      "APIUrl": ""
    },
    "Trello": {
      "APIUrl": "https://api.trello.com/1/"
    },
    "MemoryCache": {
      "ExpirationScanMinutes": 10,
      "SizeLimit": 100
    },
    "ContextEntry": {
      "Size": 1,
      "ExpirationMinutes": 60
    }
  }
}
